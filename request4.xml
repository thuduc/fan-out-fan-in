                    <type>Close</type>
                    <currency>USD</currency>
                    <rate>0.0412</rate>
                    <href>https://finance.yahoo.com/quote/ES=F/history?p=ES=F</href>
                </market>
                <model name="model1" select="/vnml/project/model[@name='Model1']"/>
                <calculator select="/vnml/project/calculator[@name='Calculator1']"/>
