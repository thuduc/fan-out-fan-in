            <type>Close</type>
            <currency>USD</currency>
            <rate>0.0412</rate>
            <href>https://finance.yahoo.com/quote/ES=F/history?p=ES=F</href>
        </market>
        <model name="Model1" date="2024-08-30" time="20:16:43">
            <type>Historical</type>
            <process>normal</process>
