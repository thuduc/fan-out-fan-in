            <valuation name="security">
                <market select="/vnml/project/market[1]"/>
                <model select="/vnml/project/model[1]"/>
                <calculator select="/vnml/project/calculator"/>
                <analytics>
                    <evaluation>
                        <date>2025-09-17</date>
                    </evaluation>
                    <price>
                        <amount>1.25</amount>
                    </price>
                    <accruedInterest>
