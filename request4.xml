                    </evaluation>
                    <price>
                        <amount>1.25</amount>
                    </price>
                </analytics>
                <instrument select="."/>
            </valuation>
        </group>
        <group name="Group3">
            <valuation name="future" use="vn:link(/vnml/project/group[2], valuation)">
                <market select="/vnml/project/market[1]"/>
                <model select="/vnml/project/model[1]"/>
                <calculator select="/vnml/project/calculator"/>
                <analytics>
                    <evaluation>
                        <date>2025-10-17</date>
                    </evaluation>
                    <previousPrice>
                        <amount select="./analytics/price/amount"/>
                    </previousPrice>
                    <price>
                        <amount>1.25</amount>
                    </price>
                </analytics>
                <instrument select="./instrument"/>
            </valuation>
        </group>
